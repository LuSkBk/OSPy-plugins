#: plugins/cli_control/__init__.py:84
msgid "CLI Control is enabled."
msgstr "Ovládací prvok CLI je zapnutý."

#: plugins/cli_control/__init__.py:91
msgid "CLI Control plug-in"
msgstr "CLI doplnok"

#: plugins/cli_control/__init__.py:95
msgid "CLI Control is disabled."
msgstr "Ovládací prvok CLI je vypnutý."

#: plugins/cli_control/__init__.py:125 plugins/cli_control/__init__.py:130
msgid "Command"
msgstr "Prikaz"

#: plugins/cli_control/templates/cli_control.html:3
#: plugins/cli_control/templates/cli_control.html:21
msgid "CLI Control settings"
msgstr "Nastavenia ovládania CLI"

#: plugins/cli_control/templates/cli_control.html:22
msgid ""
"This plugin can be used to control devices such as remote RF units via Linux "
"commands."
msgstr ""
"Tento plugin môže byť použitý na ovládanie zariadení, ako sú vzdialené RF "
"jednotky cez Linux príkazy."

#: plugins/cli_control/templates/cli_control.html:23
msgid ""
"Enter command line commands to be issued when a station is to be started or "
"stopped."
msgstr ""
"Zadajte príkazy príkazového riadka, ktoré sa vydajú pri spustení alebo "
"zastavení stanice."

#: plugins/cli_control/templates/cli_control.html:24
msgid ""
"For example a command might look like: wget http://xxx.xxx.xxx.xxx/relay1on"
msgstr ""
"Napríklad príkaz môže vyzerať takto: wget http://xxx.xxx.xxx.xxx/relay1on"

#: plugins/cli_control/templates/cli_control.html:25
msgid ""
"Leave fields blank for any stations not to be controlled from the command "
"line."
msgstr ""
"Nechajte polia prázdne pre všetky stanice, ktoré sa neovládajú z príkazového "
"riadka."

#: plugins/cli_control/templates/cli_control.html:30
msgid "Use CLI Control"
msgstr "Použiť doplnok"

#: plugins/cli_control/templates/cli_control.html:36
msgid "Station"
msgstr "Stanice"

#: plugins/cli_control/templates/cli_control.html:37
msgid "Start Command"
msgstr "Príkaz štart"

#: plugins/cli_control/templates/cli_control.html:38
msgid "Stop Command"
msgstr "Príkaz stop"

#: plugins/cli_control/templates/cli_control.html:53
msgid "Status"
msgstr "Stav"

#: plugins/cli_control/templates/cli_control.html:66
msgid "Submit"
msgstr "Odoslať"

#: plugins/cli_control/templates/cli_control.html:67
msgid "Cancel"
msgstr "Zrušiť"
